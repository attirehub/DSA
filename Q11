#include <stdio.h>
#include <string.h>

int main() {
    int n, i, low, high, mid, found = 0;
    char names[50][50], search[50];

    printf("Enter number of names: ");
    scanf("%d", &n);

    printf("Enter %d names in sorted order:\n", n);
    for (i = 0; i < n; i++)
        scanf("%s", names[i]);

    printf("Enter name to search: ");
    scanf("%s", search);

    low = 0;
    high = n - 1;

    while (low <= high) {
        mid = (low + high) / 2;
        int cmp = strcmp(search, names[mid]);

        if (cmp == 0) {
            printf("Name '%s' found at position %d.\n", search, mid + 1);
            found = 1;
            break;
        } else if (cmp > 0)
            low = mid + 1;
        else
            high = mid - 1;
    }

    if (!found)
        printf("Name '%s' not found in the list.\n", search);

    return 0;
}
